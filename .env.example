# Gemini API (required for chat)
# Get at https://aistudio.google.com/apikey
REACT_APP_GEMINI_API_KEY=your_gemini_api_key

# MongoDB Atlas connection string (required)
# Format: mongodb+srv://USER:PASSWORD@CLUSTER.mongodb.net/
REACT_APP_MONGODB_URI=your_mongodb_connection_string

# OpenAI API (optional: for transcript fallback via Whisper when YouTube captions fail)
# Install: npm install openai
OPENAI_API_KEY=

# Replicate API (optional fallback for image generation when Gemini fails)
# Get at https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=

# YouTube Data API v3 key (required for YouTube Channel Download)
# Get at https://console.cloud.google.com/apis/credentials (enable YouTube Data API v3)
# Add to .env as REACT_APP_YOUTUBE_API_KEY or YOUTUBE_API_KEY
REACT_APP_YOUTUBE_API_KEY=your_youtube_api_key
